import pandas as pd
import numpy as np
df=pd.read_csv('sales.csv')
df.loc[df['date'].str.endswith('5'),'price']=np.NaN
df['Выручка']=df['price']*df['quantity']
df.drop(['price', 'quantity'], axis= 1 , inplace= True ) 
df.rename(columns = {'date':'Дата', 'item':'Наименование'}, inplace = True )
print(df[23:44])
